user:Actor
clientUI:Mobile App OR Webpage
httpHandler:Web Server
webpageCreator:Web Server
dbconnector:Web Server
accounts:DBMS

[c:alt Registration successful]
	user:clientUI.Register
	clientUI:successful=httpHandler.Register(Data)
	httpHandler:confirmed=webpageCreator.request(Data)
	webpageCreator:success=dbconnector.addAccount(Data)
	dbconnector:insert performed=accounts.INSERT INTO ...
	clientUI:user.Home Page
	clientUI:stop
	--[Registration unsuccessful]
	user:clientUI.Register
	clientUI:aborted(Reasons Page)=httpHandler.Register(Data)
	httpHandler:denied(WebPage with Reasons)=webpageCreator.request(Data)
	webpageCreator:denied(Reasons)=dbconnector.addAccount(Data)
	dbconnector:Replicated OR Invalid data=accounts.INSERT INTO ...
	clientUI:user.RegisterPage(Fields to change)
	clientUI:stop
[/c]
